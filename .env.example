# Database Configuration
POSTGRES_DB=your_db_name
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_PORT=5432
HOST_DB=postgres

# PgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=your_admin_password

# Airflow Database Connection
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://your_username:your_password@postgres:5432/your_db_name

# Airflow Secret Keys (Required - Generate your own secure keys)
AIRFLOW__CORE__SECRET_KEY=your_secret_key_here_32_chars_minimum
AIRFLOW__WEBSERVER__SECRET_KEY=your_webserver_secret_key_32_chars_minimum
AIRFLOW__CORE__FERNET_KEY=your_fernet_key_here_44_chars_base64_encoded=

# Additional Airflow Configuration
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__LOGGING__LOGGING_LEVEL=INFO
AIRFLOW__WEBSERVER__AUTHENTICATE=False